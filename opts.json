{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "workspace": {
        "library": [
            "$VIMRUNTIME",
            "$VIMRUNTIME/lua",
            "./lua",
            "${3rd}/luv/library"
        ]
    },
    "runtime": {
        "version": "LuaJIT",
        "path": [
            "./?.lua",
            "./lua/?.lua",
            "./lua/?/init.lua",
            "./lua/?/?.lua"
        ]
    }
}
